package orientacaoobjetos;

public class MoldeAviao {

	public String cor;
	public int tamanho;
	public int capacidade;
	public String destino;

	
	
	public String getCorAviao()
	{
		String corAviao = cor;
		return corAviao;
	}
	
	public int getTamanhoAviao()
	{
		int tamanhoAviao = tamanho;
		return tamanhoAviao;
	}
	
	public int getCapacidadeAviao()
	{
		int capacidadeAviao = capacidade;
		return capacidadeAviao
	}
	
	public int getDestinoAviao()
	{
		int destinoAviao = destino;
		return destinoAviao;
	}
}
